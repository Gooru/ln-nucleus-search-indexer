{
   "settings":{
      "index":{
         "analysis":{
            "analyzer":{
               "keyword_lowercase":{
                  "type":"custom",
                  "tokenizer":"keyword",
                  "filter":[
                     "lowercase",
                     "trim"
                  ]
               },
               "gooru_kstem":{
                  "type":"custom",
                  "tokenizer":"standard",
                  "filter":[
                     "lowercase",
                     "stop",
                     "kstem"
                  ]
               }
            }
         }
      },
      "number_of_shards":"1",
      "refresh_interval":"1s"
   },
   "mappings":{
         "properties":{
            "id":{
               "type":"keyword"
            },
            "name":{
               "type":"text",
               "analyzer":"standard",
               "fields":{
                  "nameSnowball":{
                     "type":"text",
                     "analyzer":"snowball"
                  },
                  "nameKStem":{
                     "type":"text",
                     "analyzer":"gooru_kstem"
                  }
               }
            },
            "description":{
               "type":"text"
            },
            "tenantType":{
               "type":"keyword"
            },
            "createdAt":{
               "type":"date",
               "format":"dateOptionalTime"
            },
            "updatedAt":{
               "type":"date",
               "format":"dateOptionalTime"
            },
            "indexUpdatedTime":{
               "type":"date",
               "format":"dateOptionalTime"
            },
            "parentTenantId":{
               "type":"keyword"
            },
            "contentVisibility":{
               "type":"keyword"
            },
            "classVisibility":{
               "type":"keyword"
            },
            "userVisibility":{
               "type":"keyword"
            },
            "fcVisibility":{
               "type":"keyword"
            },
            "parentTenantIds":{
               "type":"keyword"
            },
            "parentTenantFCVisibility":{
               "type":"keyword"
            }
         }
      }
}            