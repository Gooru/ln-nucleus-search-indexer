{
   "settings":{
      "index":{
         "analysis":{
            "analyzer":{
               "keyword_lowercase":{
                  "type":"custom",
                  "tokenizer":"keyword",
                  "filter":[
                     "lowercase",
                     "trim"
                  ]
               }
            }
         }
      }
   },
   "mappings":{
         "properties":{
            "definitions":{
               "type":"text",
               "analyzer":"keyword_lowercase"
            },
            "indexId":{
               "type":"text"
            },
            "indexType":{
               "type":"text"
            },
            "indexUpdatedDate":{
               "type":"date",
               "format":"dateOptionalTime"
            },
            "type":{
               "type":"text",
               "analyzer":"keyword_lowercase"
            },
            "word":{
               "type":"text",
               "analyzer":"keyword_lowercase"
            }
         }
      }
}