{
   "settings":{
      "index":{
         "analysis":{
            "analyzer":{
               "keyword_lowercase":{
                  "type":"custom",
                  "tokenizer":"keyword",
                  "filter":[
                     "lowercase",
                     "trim"
                  ]
               },
               "partial_split_word":{
                  "type":"custom",
                  "tokenizer":"standard",
                  "filter":[
                     "lowercase",
                     "trim",
                     "name_ngrams",
                     "split_words_by_special_character"
                  ]
               },
               "gooru_kstem":{
                  "type":"custom",
                  "filter":[
                     "standard",
                     "lowercase",
                     "stop",
                     "kstem"
                  ],
                  "tokenizer":"standard"
               },
               "full_text":{
                  "filter":[
                     "lowercase"
                  ],
                  "type":"custom",
                  "tokenizer":"keyword"
               }
            },
            "filter":{
               "name_ngrams":{
                  "side":"front",
                  "max_gram":30,
                  "min_gram":1,
                  "type":"edgeNGram"
               },
               "split_words_by_special_character":{
                  "type":"word_delimiter",
                  "generate_word_parts":"true",
                  "generate_number_parts":"true",
                  "catenate_words":"false",
                  "catenate_numbers":"false",
                  "catenate_all":"false",
                  "split_on_case_change":"false",
                  "split_on_numerics":"false",
                  "stem_english_possessive":"false",
                  "preserve_original":"true"
               }
            }
         }
      }
   },
   "mappings":{
      "taxonomy":{
         "properties":{
            "all":{
               "type":"text"
            },
            "indexType":{
               "type":"text"
            },
            "indexUpdatedDate":{
               "type":"date",
               "format":"dateOptionalTime"
            },
            "id":{
               "type":"keyword"
            },
            "code":{
               "type":"text",
               "analyzer":"keyword_lowercase"
            },
            "codeType":{
               "type":"keyword"
            },
            "title":{
               "type":"text",
               "analyzer":"standard",
               "fields":{
                  "titleSnowball":{
                     "type":"text",
                     "analyzer":"snowball"
                  },
                  "titleKStem":{
                     "type":"text",
                     "analyzer":"gooru_kstem"
                  }
               },
               "copy_to":[
                  "all"
               ]
            },
            "description":{
               "type":"text",
               "analyzer":"standard",
               "fields":{
                  "descriptionKStem":{
                     "type":"text",
                     "analyzer":"gooru_kstem"
                  },
                  "descriptionSnowball":{
