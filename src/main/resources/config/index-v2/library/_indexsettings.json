{
   "settings":{
      "index":{
         "analysis":{
            "analyzer":{
               "keyword_lowercase":{
                  "type":"custom",
                  "tokenizer":"keyword",
                  "filter":[
                     "lowercase",
                     "trim"
                  ]
               }
            }
         }
      }
   },
   "mappings":{
      "library":{
         "properties":{
            "collections":{
               "type":"keyword"
            },
            "id":{
               "type":"integer"
            },
            "indexUpdatedDate":{
               "type":"date",
               "format":"dateOptionalTime"
            },
            "organization":{
               "properties":{
                  "code":{
                     "type":"keyword"
                  },
                  "name":{
                     "type":"keyword"
                  },
                  "partyUid":{
                     "type":"keyword"
                  }
               }
            },
            "partyPermissions":{
               "type":"nested",
               "properties":{
                  "name":{
                     "type":"keyword"
                  },
                  "type":{
                     "type":"keyword"
                  },
                  "uId":{
                     "type":"keyword"
                  }
               }
            },
            "quizzes":{
               "type":"keyword"
            },
            "resourcesIdList":{
               "type":"text"
            },
            "simple_collections":{
               "type":"keyword"
            },
            "simple_quizzes":{
               "type":"keyword"
            },
            "type":{
               "type":"keyword"
            }
         }
      }
   }
}
