{
   "index":{
      "analysis":{
         "filter":{
            "autocomplete_filter":{
               "type":"edge_ngram",
               "min_gram":2,
               "max_gram":20,
               "token_chars":[
                  "letter",
                  "digit"
               ]
            }
         },
         "analyzer":{
            "autocomplete":{
               "type":"custom",
               "tokenizer":"standard",
               "filter":[
                  "lowercase",
                  "autocomplete_filter"
               ]
            },
            "keyword_lowercase":{
               "type":"custom",
               "tokenizer":"keyword",
               "filter":[
                  "lowercase",
                  "trim"
               ]
            }
         }
      }
   }
}
